package satiwan;

package practice;

import practice.StacksPractice;

public class StacksPractice {
	
	int top;
	int size;
	
	int a [] = new int[5];
	
	public StacksPractice(){
		top=-1;
		size=a.length;
	}
	
	
	public void push(int d){
		if(top<0){
			System.out.println("stack is full");
		}
		else
		{
			top++;
			a[top]=d;
			
		}
	}
	
	public void display(){
		for(int i=top;i<=size;i++)
		System.out.println(a[top]);
		
	}

}

	


