package practice;

import java.io.*;

public class fileExample {
	public static void main(String[] args) {
		
		int i ;
FileOutputStream fout =  new FileOutputStream("./myfile.txt",true);
		
		String s = "tata";
		char ch[]=s.toCharArray();
		for(i=0;i<s.length();i++){
			fout.write(ch[i]);
			fout.close();
		}
		
	}
}
